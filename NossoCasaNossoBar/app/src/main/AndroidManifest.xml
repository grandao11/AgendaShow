<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vandin.nossocasanossobar">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity" />
        <activity android:name=".activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.CadClienteActivity" />
        <activity android:name=".activities.CadPedidoActivity" />
        <activity android:name=".activities.ListaClienteActivity" />
        <activity android:name=".activities.ListaPedidoActivity" />
        <activity android:name=".activities.CadBebidaActivity" />
        <activity android:name=".activities.ListaBebidaActivity" />
        <activity android:name=".activities.CadAperitivoActivity" />
        <activity android:name=".activities.ListaAperitivoActivity"></activity>
    </application>

</manifest>